{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSON schema for order create response",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "order": {
      "type": "object",
      "properties": {
        "number": {
          "type": "number"
        }
      },
      "required": [
        "number"
      ]
    },
    "success": {
      "type": "boolean"
    }
  },
  "required": [
    "name",
    "order",
    "success"
  ]
}