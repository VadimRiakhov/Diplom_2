{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSON schema for orders get by user response",
  "type": "object",
  "properties": {
    "success": {
      "type": "boolean"
    },
    "orders": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "ingredients": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "_id": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "number": {
            "type": "number"
          },
          "createdAt": {
            "type": "string"
          },
          "updatedAt": {
            "type": "string"
          }
        },
        "required": [
          "ingredients",
          "_id",
          "status",
          "number",
          "createdAt",
          "updatedAt"
        ]
      }
    },
    "total": {
      "type": "number"
    },
    "totalToday": {
      "type": "number"
    }
  },
  "required": [
    "success",
    "orders",
    "total",
    "totalToday"
  ]
}